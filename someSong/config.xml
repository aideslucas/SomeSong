    <plugin name="ionic-plugin-deploy" spec="^0.6.7" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1" />
    <plugin name="phonegap-plugin-push" spec="^1.10.4">
        <variable name="SENDER_ID" value="655905548469" />
    </plugin>
